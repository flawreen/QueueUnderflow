@{

}

<div class="d-flex flex-column align-items-center justify-content-center">

    <h1 class="display-6 my-3">User panel</h1>
    <div class="list-group my-5" style="width:30rem;">
        @foreach(QueueUnderflow.Models.ApplicationUser user in ViewBag.UsersList)
        {
            <a href="/Users/Show/@user.Id" class="list-group-item list-group-item-action">
                <div class="d-flex w-100 justify-content-between">
                    <h5 class="mt-1 h5"><i class="bi bi-person-gear"></i> @user.UserName</h5>
                    <i class="mt-1 bi bi-three-dots"></i>
                </div>
            </a>
        }
    </div>

</div>
